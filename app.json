{
  "expo": {
    "name": "TouristGuide",
    "slug": "TouristGuide",
    "scheme": "touristguide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Tg.png",
    "newArchEnabled": true,
    "experiments": {
      "newArchEnabled": true
    },
    "linking": {
      "prefixes": ["votreapp://", "https://votreapp.com"],
