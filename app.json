    "extra": {
      "eas": {
        "projectId": "30359f0f-8543-427a-b39f-89a8313196cd"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/30359f0f-8543-427a-b39f-89a8313196cd"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
